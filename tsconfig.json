{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "downlevelIteration": true,
    "removeComments": true,
    "sourceMap": true,
    "baseUrl": "src",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules"
  ]
}